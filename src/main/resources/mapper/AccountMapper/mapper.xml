<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.coffee.account.AccountMapper">

    <select id="GetAllAccount" resultType="com.example.coffee.account.domain.Account$Member">
        select * from member
    </select>

    <select id="ChooseBuyerSelector" resultType="com.example.coffee.account.domain.Account$ChooseBuyerSelector">
        select name from member
        where id = 1
    </select>

</mapper>